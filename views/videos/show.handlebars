<div class="contents-container">
  <div class="video-card">

    <div class="video-title">
      <h1 id="title">{{video.title}}</h1>
    </div>

    <iframe width="420" height="315" id="iframe" class="video-player" src="{{ video.url }}" frameborder="0" allowfullscreen>not supported</iframe>

    <div class="video-description">
      <p id="description">{{video.description}}</p>
    </div>
    <div class="edit-delete-container">
        <a href="/videos/{{video._id}}/edit" class="add-video-button" id="edit">EDIT</a>
        <a href="/videos/{{video._id}}/delete" class="add-video-button" id="delete">DELET</a>
    </div>
  </div>
</div>