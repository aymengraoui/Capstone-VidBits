<div id="videos-container" class="contents-container">
    {{#each videos as |video|}}
        <div class="video-card">
            
            <div class="video-title">
                <a id="show-video" href="/videos/{{ video._id }}">{{video.title}}</a>
            </div>
            <iframe id="iframe" class="video-player" src="{{ video.url }}" frameborder="0" allowfullscreen></iframe>
        </div>
    {{/each}}
</div>
<a href="/videos/create" class="add-video-button" id="create-videos">Create Video</a>
